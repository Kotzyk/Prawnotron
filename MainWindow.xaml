<Window x:Class="Prawnotron.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prawnotron"
        mc:Ignorable="d"
        Title="MainWindow" Height="553.4" Width="640">
    <Window.Background>
        <ImageBrush>
            <ImageBrush.ImageSource>
                <BitmapImage UriSource="/Resources/tlo.jpg"/>
            </ImageBrush.ImageSource>
        </ImageBrush>
    </Window.Background>
    <Button x:Name="button" HorizontalAlignment="Left" Margin="237,389,0,0" VerticalAlignment="Top" Width="163" Height="71" Click="button_Click" BorderThickness="0,0,0,1" Foreground="{x:Null}">
        <Image Source="/Resources/BlueSquareButton.png" Height="71"/>
    </Button>
</Window>
